    <script>
        $(document).ready(function () {

            var marital = $("#ContentPlaceHolder1_txtMaritalStatus").val();

            // alert(marital);


            if (marital == 'Married') {

                $('#dvSpouseBlick').show();
                // document.getElementById('dvSpouseBlick').style.display = "block";
            }
            else {

                $('#dvSpouseBlick').hide();
                // document.getElementById('dvSpouseBlick').style.display = "none";
            }


        });


    </script>


