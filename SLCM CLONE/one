                        <script>

                            function SpecialCherValidateWithSpace(ob) {
                                var invalidChars = /[^a-zA-Z0-9 ]+/
                                if (invalidChars.test(ob.value)) {
                                    ob.value = ob.value.replace(invalidChars, "");
                                }
                            };

                            function isNumber(ob) {
                                var invalidChars = /[^0-9]+/gi
                                if (invalidChars.test(ob.value)) {
                                    ob.value = ob.value.replace(invalidChars, "");
                                }
                            };

                            function isNumber(ob) {
                                var invalidChars = /[^0-9]+/gi
                                if (invalidChars.test(ob.value)) {
                                    ob.value = ob.value.replace(invalidChars, "");
                                }
                            };

                            function funViewMobilePanal() {

                                var CurrentMoible = document.getElementById("ContentPlaceHolder1_txtPresentMobile").value;
                                var OldMobile = document.getElementById("ContentPlaceHolder1_hdnCompareMobile").value;
                                if (CurrentMoible !== "") {
                                    if (CurrentMoible != OldMobile) {
                                        document.getElementById("divViewMobileOTP").style.display = "Block";
                                        document.getElementById("divViewMobileOTP2").style.display = "Block";
                                        return false;
                                    }
                                    else {
                                        document.getElementById("divViewMobileOTP").style.display = "none";
                                        document.getElementById("divViewMobileOTP2").style.display = "none";
                                        return false;
                                    }
                                }

                            }

                            function funViewEmailPanal() {
                                var CurrentEmail = document.getElementById("ContentPlaceHolder1_txtPesentEmail").value;
                                var OldEmail = document.getElementById("ContentPlaceHolder1_hdnCompareEmail").value;
                                if (CurrentEmail != OldEmail) {
                                    document.getElementById("divEmailViewOTP").style.display = "Block";
                                    document.getElementById("divEmailViewOTP2").style.display = "Block";
                                    return false;
                                }

                                if (CurrentEmail == "") {
                                    document.getElementById("divEmailViewOTP").style.display = "none";
                                    document.getElementById("divEmailViewOTP2").style.display = "none";
                                    return false;
                                }
                            }

                            var vOTPForMobile = "";
                            function otpOpenPopupMobile() {

                                var vMoible = document.getElementById("ContentPlaceHolder1_txtPresentMobile").value;
                                if (vMoible == "") {
                                    customAlert("e", "Enter Mobile No.");
                                    return false;
                                }

                                if (vMoible.length < 10) {
                                    customAlert("e", "Enter Valid Mobile No.");
                                    return false;
                                }

                                if (vMoible.length > 9) {
                                    $.ajax({
                                        type: "post",
                                        url: "StudentProfile.aspx/sentOTPMessage",
                                        data: "{strMobileNo:'" + vMoible + "'}",
                                        contentType: "application/json; charset=utf-8",
                                        dataType: "json",
                                        success: function (msg) {
                                            if (msg.d != "") {
                                                vOTPForMobile = msg.d;
                                                document.getElementById("lblbtnPresentMobileSendOTP").style.display = 'block';
                                                document.getElementById("lblbtnPresentMobileSendOTP").innerHTML = "OTP has been sent on mobile.";// + msg.d;
                                                document.getElementById("lnkSendMobileOTP").innerHTML = "Resend OTP";
                                                //$("#txtPreOTP").val(msg.d); //Manju Unikul added to display OTP                                                        
                                                //customAlert("s", "Your OTP is: " + msg.d);
                                                return false;
                                            }
                                            else {
                                                customAlert("e", "Sorry ! Your Message is not send ! ");
                                                return false;
                                            }
                                        }
                                    });
                                    return false;
                                }

                                return false;
                            }
                            function PresentMobileVerify() {

                                var btnVerify = document.getElementById("lnkVerifyMobilOtp").innerHTML;
                                if (btnVerify != "Verify") {
                                    return false;
                                }

                                var vPreMobileForVerify = document.getElementById("ContentPlaceHolder1_txtPreOTP").value;

                                if (vPreMobileForVerify == "") {
                                    customAlert("e", "Enter OTP Code.");
                                    return false;
                                }

                                if (vOTPForMobile == vPreMobileForVerify) {
                                    document.getElementById("lblbtnPresentMobileSendOTP").innerHTML = "";
                                    document.getElementById("lnkVerifyMobilOtp").innerHTML = "Verified Successfully";
                                    document.getElementById('lnkSendMobileOTP').style.visibility = 'hidden';
                                    document.getElementById("ContentPlaceHolder1_hdnCompareMobile").value = document.getElementById("ContentPlaceHolder1_txtPresentMobile").value;
                                    return false;
                                }
                                else {
                                    customAlert("e", "OTP is invalid try again");
                                    return false;
                                }
                                return false;
                            }

                            var vOTPForeMail = "";
                            function otpOpenPopupEmail() {

                                var vEmailID = document.getElementById("ContentPlaceHolder1_txtPesentEmail").value;
                                if (vEmailID == "") {
                                    customAlert("e", "Enter Email Id");
                                    return false;
                                }

                                var reg = /^\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/;
                                if (!reg.test(vEmailID)) {
                                    customAlert("e", "Enter Valid Email Id.");
                                    return false;
                                }

                                $.ajax({
                                    type: "post",
                                    url: "StudentProfile.aspx/sentOTPMessageForMail",
                                    data: "{strMail:'" + vEmailID + "'}",
                                    contentType: "application/json; charset=utf-8",
                                    dataType: "json",
                                    success: function (msg) {
                                        if (msg.d != "") {
                                            vOTPForeMail = msg.d;
                                            document.getElementById("lbltxtStudentEmailID").style.display = 'block';
                                            document.getElementById("lbltxtStudentEmailID").innerHTML = "OTP has been sent on e-mail";// + msg.d;
                                            document.getElementById("sendEmailOTP").innerHTML = "Resend OTP";
                                            //Manju Unikul added to display OTP
                                            //$("#txtPreEmailVerify").val(msg.d);
                                            //customAlert("s", "Your OTP is: " + msg.d);
                                            return false;

                                        }
                                        else {
                                            customAlert("e", "Sorry ! Your Message is not send !");
                                            return false;
                                        }

                                    }
                                });
                                return false;
                            }

                            function PresentEmailVerify() {

                                var vPreEmailForVerify = document.getElementById("ContentPlaceHolder1_txtPreEmailVerify").value;
                                if (vPreEmailForVerify == "") {
                                    customAlert("e", "Enter OTP Code.");
                                    return false;
                                }

                                if (vOTPForeMail == vPreEmailForVerify) {
                                    document.getElementById("lbltxtStudentEmailID").innerHTML = "";
                                    document.getElementById("VerifyEmailOTP").innerHTML = "Verified Successfully";
                                    document.getElementById('sendEmailOTP').style.visibility = 'hidden';
                                    return false;
                                }
                                else {
                                    customAlert("e", "OTP is invalid try again.");
                                    return false;
                                }
                                return false;
                            }


                            function funUpdateValidation() {

                                var vMobileDiv = document.getElementById("divViewMobileOTP").style.display;
                                var vMobileVerText = document.getElementById("lnkVerifyMobilOtp").innerHTML;
                                if (vMobileDiv == 'block' && vMobileVerText == "Verify") {
                                    customAlert("e", "Please verify your mobile no.");
                                    return false;
                                }

                                var vEmailDiv = document.getElementById("divEmailViewOTP").style.display;
                                var vEmailVerText = document.getElementById("VerifyEmailOTP").innerHTML;
                                if (vEmailDiv == 'block' && vEmailVerText == "Verify") {
                                    customAlert("e", "Please verify your Email id.");
                                    return false;
                                }


                                var vMobileNo = document.getElementById("ContentPlaceHolder1_txtPresentMobile").value;
                                var vEmail = document.getElementById("ContentPlaceHolder1_txtPesentEmail").value;
                                var vAddressOne = document.getElementById("ContentPlaceHolder1_txtPresentAddressLine1").value;
                                var vAddressTwo = document.getElementById("ContentPlaceHolder1_txtPresentAddressLine2").value;
                                var vAddressThree = document.getElementById("ContentPlaceHolder1_txtPresentAddressLine3").value;
                                var vPlace = document.getElementById("ContentPlaceHolder1_txtPresentPlace").value;
                                var vCountry = $('select[id$="ddlPresentCountry"] option:selected').val();
                                var vState = $('select[id$="ddlPresentState"] option:selected').val();
                                var vCommunication = $('select[id$="ddlCommunication"] option:selected').val();

                                var vPincode = document.getElementById("ContentPlaceHolder1_txtPresentPincode").value;

                                if (vMobileNo == "") {
                                    customAlert("e", "Enter Mobile No.");
                                    return false;
                                }

                                if (vMobileNo.length < 10) {
                                    customAlert("e", "Enter Valid Mobile No.");
                                    return false;
                                }


                                if (vEmail == "") {
                                    customAlert("e", "Enter email.");
                                    return false;
                                }

                                var reg = /^\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/;
                                if (!reg.test(vEmail)) {
                                    customAlert("e", "Enter Valid Email Id.");
                                    return false;

                                }

                                vAddressOne = vAddressOne.replace(/'/g, '"');
                                if (vAddressOne == "") {
                                    customAlert("e", "Enter address 1");
                                    return false;

                                }

                                vAddressTwo = vAddressTwo.replace(/'/g, '"');
                                if (vAddressTwo == "") {
                                    customAlert("e", "Enter address 2.");
                                    return false;
                                }

                                vAddressThree = vAddressThree.replace(/'/g, '"');
                                if (vAddressThree == "") {
                                    customAlert("e", "Enter address 3.");
                                    return false;
                                }

                                vPlace = vPlace.replace(/'/g, '"');
                                if (vPlace == "") {
                                    customAlert("e", "Enter Place.");
                                    return false;
                                }

                                if (vCountry == "" || vCountry == "Select Country") {
                                    customAlert("e", "Select Country.");
                                    return false;
                                }

                                var vStudentStatus = document.getElementById("ContentPlaceHolder1_hdnStudentStatus").value;

                                if (vStudentStatus == "GENERAL") {
                                    if (vState == "Select State" || vState == "") {
                                        customAlert("e", "Select State.");
                                        return false;
                                    }

                                    if (vPincode == "") {
                                        customAlert("e", "Enter pincode.");
                                        return false;
                                    }

                                    if (vPincode.length < 6) {
                                        customAlert("e", "Enter valid pincode.");
                                        return false;
                                    }
                                }



                                if (vCommunication == "Select Communication" || vCommunication == "") {
                                    customAlert("e", "Select Communication Address.");
                                    return false;
                                }


                                if (vEmail !== "") {
                                    vEmail = vEmail.replace(/'/g, '"');
                                }


                                $.ajax({
                                    type: "post",
                                    url: "StudentProfile.aspx/StudentUpdateDetailsUpdateJQuery",
                                    data: "{AlternateEmailAddress:'" + vMobileNo
                                        + "', OfficialCorrespoMobileNo:'" + vEmail
                                        + "', Address3:'" + vAddressOne
                                        + "', Address4:'" + vAddressTwo
                                        + "', CorCity:'" + vPlace
                                        + "', CorState:'" + vState
                                        + "', CorCountryCode:'" + vCountry
                                        + "', CorPostCode:'" + vPincode
                                        + "', CommunicationAddress:'" + vCommunication
                                        + "', AddressTo:'" + vAddressThree
                                        + "'}",
                                    contentType: "application/json; charset=utf-8",
                                    dataType: "json",
                                    success: function (msg) {
                                        if (msg.d == "OK") {
                                            customAlert("s", "Record has been updated successfully.");
                                            return false;
                                        }
                                        else {
                                            customAlert("e", "Error.");
                                            return false;
                                        }
                                    }
                                });

                                return false;
                            }
                            $(document).ready(function () {

                                var vStudentStatus = document.getElementById("ContentPlaceHolder1_hdnStudentStatus").value;
                                if (vStudentStatus == "GENERAL") {
                                    $("#lblddlPresentState").show();
                                    $("#lblPincode").show();
                                }
                                else {
                                    $("#lblddlPresentState").hide();
                                    $("#lblPincode").hide();
                                }
                                document.getElementById("li5").className = "tab-1 tabs active";

                                return false;
                            });

                            function checkFileFormate() {
                                var vFileName = document.getElementById("ContentPlaceHolder1_fuUpload").value;
                                var vFileFormat = vFileName.split('.').pop();
                                if ((vFileFormat.toUpperCase() !== "JPG") && (vFileFormat.toUpperCase() !== "JPEG") && (vFileFormat.toUpperCase() !== "PNG") && (vFileFormat.toUpperCase() !== "GIF") && (vFileFormat.toUpperCase() !== "DOC") && (vFileFormat.toUpperCase() !== "DOCX") && (vFileFormat.toUpperCase() !== "PDF")) {
                                    customAlert("e", "Please select Image,Word or PDF file.");
                                    return false;
                                }
                            }

                        </script>

                        <script type="text/javascript">
                            $(function () {
                                $('#btnUploadProfile').click(function () {
                                    var vFile = document.getElementById("FileUpload1").value;
                                    if (vFile == "") {
                                        customAlert("e", "Select attachment file.");
                                        return false;
                                    }

                                    var vDoctype = $('#documentType option:selected').val();
                                    if (vDoctype == "" || vDoctype == "0") {
                                        customAlert("e", "Select Document Type.");
                                        return false;
                                    }

                                    var imgSize = $("#FileUpload1").get(0).files[0].size;

                                    var MbSize = imgSize / 1024;
                                    if (MbSize > 2048) {
                                        customAlert("e", "File size should not be more than 2MB.");
                                        return false;
                                    }

                                    var fileUpload = $("#FileUpload1").get(0);
                                    var files = fileUpload.files;
                                    var test = new FormData();
                                    var vFileName = "";

                                    var docType = $('#documentType  option:selected').val();
                                    for (var i = 0; i < files.length; i++) {
                                        test.append(files[i].name, files[i]);
                                        vFileName = files[i].name;
                                    }

                                    var vFileFormat = vFileName.split('.').pop();
                                    if ((vFileFormat.toUpperCase() !== "JPG") && (vFileFormat.toUpperCase() !== "JPEG") && (vFileFormat.toUpperCase() !== "PNG") && (vFileFormat.toUpperCase() !== "GIF") && (vFileFormat.toUpperCase() !== "DOC") && (vFileFormat.toUpperCase() !== "DOCX") && (vFileFormat.toUpperCase() !== "PDF")) {
                                        customAlert("e", "Please select Image,Word or PDF file.");
                                        return false;
                                    }

                                    //return false;
                                    document.getElementById('btnUploadProfile').disabled = true;
                                    //  $('#btnUpload').prop('disabled','true'); //Disabled Button To restrict Multiple Click
                                    $.ajax({
                                        url: "UploadImageFromProfile.ashx?docuType=" + docType + "&docName='" + $('#documentType  option:selected').text() + "'",
                                        type: "POST",
                                        contentType: false,
                                        processData: false,
                                        data: test,
                                        success: function (result) {

                                            $('#FileUpload1').val('');
                                            $('#secondaryUpload').val('');

                                            customAlert("s", "Document Uploaded Successfully");
                                            $("#documentType option[value='" + vDoctype + "']").remove();
                                            document.getElementById("divAttach").style.display = "block";
                                            document.getElementById("sample-table-1").insertRow(-1).innerHTML = "";
                                            document.getElementById("sample-table-1").insertRow(-1).innerHTML = result
                                            document.getElementById('btnUploadProfile').disabled = false;
                                        },
                                        error: function (err) {
                                            customAlert("e", err.statusText);
                                            return false;
                                        }
                                    });
                                });
                            })

                            $(document).ready(function () {
                                $.ajax({
                                    type: "post",
                                    url: "StudentProfile.aspx/viewAllUploadedDocument",
                                    data: "{strStudentCode:'" + "" + "'}",
                                    contentType: "application/json; charset=utf-8",
                                    dataType: "json",
                                    success: OnViewAllDocumentType,
                                    failure: function (response) {
                                        customAlert("e", "Error.");
                                        return false;
                                    },
                                    error: function (response) {
                                        customAlert("e", "Error.");
                                        return false;
                                    }
                                });

                                function OnViewAllDocumentType(response) {
                                    var xmlDoc = $.parseXML(response.d);
                                    var xml = $(xmlDoc);
                                    var customers = xml.find("TallDocument");
                                    if (customers.length > 0) {
                                        document.getElementById("divAttach").style.display = "block";
                                        $(customers).each(function () {
                                            $("#documentType option[value='" + $(this).find("DdDcumentType").text() + "']").remove();
                                            $("#sample-table-1").find("tbody").append("<tr id='" + $(this).find("Sno").text() + "'><td style='display:none'>" + $(this).find("RowId").text() + "</td><td>" + $(this).find("StudentFileName").text() + "</td><td class='text-center'>" + $(this).find("UpdateDate").text() + "</td><td class='text-center'>" + $(this).find("ApprovedType").text() + "</td><td class='text-center'><input type='button' class='btn btn-success btn-sm' title=" + $(this).find("Sno").text() + " value='Download/View' onclick='return DounloadDocumentImage(this)'></td><td class='text-center'><input type='button' class='btn btn-danger btn-sm' title=" + $(this).find("Sno").text() + " value='Delete' onclick='return DeleteDocumentImage(this)' style=' " + $(this).find("forView").text() + "'></td></tr>");
                                        });
                                    }
                                    else {

                                        //$('#txtSState').append('<option value="' + "0" + '">' + "" + '</option>');
                                    }
                                }

                            });

                            function DounloadDocumentImage(thisId) {
                                $.ajax({
                                    type: "post",
                                    url: "StudentProfile.aspx/viewAndDownloadDocument",
                                    data: "{DocumentNo:'" + thisId.title + "'}",
                                    contentType: "application/json; charset=utf-8",
                                    dataType: "json",
                                    success: function (result) {
                                        document.getElementById("ContentPlaceHolder1_btnimgView").click();
                                    },
                                    error: function (err) {
                                        customAlert("e", err.statusText);
                                        return false;
                                    }
                                });

                            }

                            function DeleteDocumentImage(thisId) {
                                var abc = thisId;
                                $.ajax({
                                    type: "post",
                                    url: "StudentProfile.aspx/DeleteDownloadDocument",
                                    data: "{DocumentNo:'" + thisId.title + "'}",
                                    contentType: "application/json; charset=utf-8",
                                    dataType: "json",
                                    success: function (response) {
                                        $("#documentType").append(response.d);
                                        $(abc).closest('tr').hide();
                                    },
                                    error: function (err) {
                                        customAlert("e", err.statusText);
                                        return false;
                                    }
                                });

                            }


                        </script>


